<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
		location="F:\Maven\repo\m2\mysql\mysql-connector-java\5.1.24\mysql-connector-java-5.1.24.jar" />
	<context id="context1">
		<property name="javaFileEncoding" value="utf-8"/>
		<!-- pagination plugin -->
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
		<plugin type="org.mybatis.generator.plugin.PaginationPGPlugin" />
		<plugin type="org.mybatis.generator.plugin.CommentPlugin" />

		<commentGenerator>
			<!-- 不生成注解信息 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
 
 		<jdbcConnection driverClass="com.mysql.Driver"
			connectionURL="jdbc:mysql://localhost:3306/chu" userId="chu"
			password="root" />

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.chu.clothes.dao"
			targetProject="chu-clothes" >
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="com.chu.clothes.dao.mapper"
			targetProject="chu-clothes" >
		</sqlMapGenerator>
		<javaClientGenerator targetPackage="com.chu.clothes.dao.mapper.xml"
			targetProject="chu-clothes" type="XMLMAPPER" >
		</javaClientGenerator>
			<table schema="chu" tableName="user" domainObjectName="User" />
	</context> 
    
</generatorConfiguration>
